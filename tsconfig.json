{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "dist",
    "target": "ES2016",
    "baseUrl": ".",
    "rootDir": ".",
    "lib": ["es2017", "es2019", "dom", "dom.iterable"],
    "paths": {
      "buildbuddy/*": ["*", "bazel-bin/*"],
      "lib/*": ["app/lib/*"],
      "*": ["node_modules/*"]
    }
  },
  "exclude": ["bazel-out", "bazel-bin"]
}
